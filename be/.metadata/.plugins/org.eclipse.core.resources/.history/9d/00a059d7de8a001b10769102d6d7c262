package com.example.demo.cmm.controller;

import org.springframework.stereotype.Controller;

@RestController
public class HomeController {
	
	@GetMapping("/")
	public String home() {
		System.out.println("====================");
		System.out.println("====Home Started====");
		System.out.println("====================");
		return "<h1>Hello world!</h1>";
	}
}